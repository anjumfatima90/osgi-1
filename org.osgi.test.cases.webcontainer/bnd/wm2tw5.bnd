Bundle-Version: 1.0
Bundle-SymbolicName: ${p}.wm2tw5
Bundle-Name: ${p}.wm2tw5
Bundle-Description: OSGi Web Container RFC 66 Compliance Test test war 5 with manifest somewhat pre-specified.  Bundle-Classpath is specified incorrectly

-sources=false

-privatepackage: ${p}.tw5.*
-wab: resources/tw5
-resourceonly=true
-removeheaders: !Manifest-Version, \
 !Bundle-Vendor, \
 !Bundle-Name, \
 !Bundle-Version, \
 !Bundle-ManifestVersion, \
 !Bundle-SymbolicName, \
 !Bundle-Description, \
 !Bundle-ClassPath, \
 *
-pom=false

-includeresource = \
	WEB-INF/lib/log.jar=log.jar, \
	WEB-INF/lib/simple.jar=simple.jar 

