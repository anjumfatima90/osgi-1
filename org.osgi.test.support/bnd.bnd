# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

javac.profile=compact2

Bundle-Description: 			General Support for OSGi JUnit based testing
Export-Package:  				org.osgi.test.support.*

-buildpath:   \
    osgi.core;maven-scope=provided;version=4.3.1, \
    osgi.cmpn; version=4.3.1, \
    org.osgi.util.converter; version=latest,\
    osgi.tck.junit-platform;version=latest,\
    org.apiguardian:apiguardian-api;version=latest

# This project is for -conditionalpackage use only
-buildrepo:
-releaserepo:
